      "import": "./dist/log/index.js",
      "types": "./dist/log/index.d.ts"
    },
    "./utils": {
      "import": "./dist/utils/index.js",
      "types": "./dist/utils/index.d.ts"
    }
  },
  "bin": {
    "next-era": "./bin/index.js"
